
public class Casa {
	
	private String sala;
	private String cozinha;
	private String quarto;
	private String banheiro;
	
	private int comodo = 2;	
	
	
	public boolean getSala(int comodo) {
		if (comodo == 1) {
			System.out.println("Voê está na sala");						
		}else {
			System.out.println("");				
		}
		return false;
			
	}
	
	public void getCozinha() {
		if (comodo == 2) {
			System.out.println("Você está na cozinha");
		}
		System.out.println("");		
	}
	
	public void getQuarto() {
		if (comodo == 3) {
			System.out.println("Você está no quarto");
		}
		System.out.println("");	
	}
	
	public void getBanheiro() {
		if (comodo == 4) {
			System.out.println("Você está no Banheiro");			
		}
		System.out.println("");		
	}
	
	

}
