	private String comodo = "sala";					// 4 - banheiro
	
	
	public String getSala(String comodo) {
		if (comodo == "sala") {
			System.out.println("Você está na sala");						
		}else {
				
			
		}
		System.out.println("");
		return "";	
			
	}
	
	public String getCozinha() {
		if (comodo == 2) {
			System.out.println("Você está na cozinha");
		}
		System.out.println("");
		return "";		
	}
	
	public String getQuarto() {
		if (comodo == 3) {
			System.out.println("Você está no quarto");
		}
		System.out.println("");
		return "";	
	}
	
	public String getBanheiro() {
		if (comodo == 4) {
			System.out.println("Você está no Banheiro");			
		}
		System.out.println("");
		return "";		
	}
	
	

}
