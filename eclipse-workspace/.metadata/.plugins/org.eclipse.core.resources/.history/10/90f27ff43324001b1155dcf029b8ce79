
public class Casa {
	
	private String sala = "sala";
	private String cozinha = "cozinha";
    private String quarto = "quarto";					// 1 - Sala
	private String banheiro = "banheiro";				// 2 - cozinha
											// 3 - quarto
	private String comodo = "cozinha";					// 4 - banheiro
	
	public String getQuarto(String comodo) {
		if (this.quarto == comodo) {
			System.out.println("Você está no quarto");
		}
		System.out.println("");
		return "";	
	}
	
	public String getBanheiro(String comodo) {
		if (this.banheiro == comodo) {
			System.out.println("Você está no Banheiro");			
		}
		System.out.println("");
		return "";		
	}
	
	public String getCozinha(String comodo) {
		if (this.cozinha == comodo) {
			System.out.println("Você está no Cozinha");			
		}
		System.out.println("");
		return "";		
	}
	
	public String getSala(String comodo) {
		if (this.sala == comodo) {
			System.out.println("Você está no Sala");			
		}
		System.out.println("");
		return "";		
	}
	
	
	

}
