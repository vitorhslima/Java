
public enum ComodoEnum {

	sair,
	cozinha,	
	banheiro,
	sala,
	quarto;
	
	public static boolean existeComodo(String comodo) {
		
		System.out.println("comodo: " + comodo);
		
		for(ComodoEnum c: ComodoEnum.values()) {
			if (!comodo.equals("sair")) {
				if (comodo.equals(c.name())) {
					return true;
				}
			}
		}
		return false;
	}
	
}
