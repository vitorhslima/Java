import java.util.Scanner;

public class testee  {
	
	public static void main(String[] args) {

		Scanner s = new Scanner(System.in);
		System.out.println("Digite o comodo que você quer ir ?");
		String comodo = s.nextLine();
		
		if (comodo == "banheiro") {
			System.out.println("Você está no " + comodo);
		}
		System.out.println("Esse comodo não é valido");
		
		
		
		
	}
	

}
