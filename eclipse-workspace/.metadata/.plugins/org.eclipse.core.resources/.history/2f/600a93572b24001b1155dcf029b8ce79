
public class Casa {
	
	private String sala;
	private String cozinha;
	private String quarto;
	private String banheiro;
	
	private int comodo = 1;	
	
	
	public String getSala() {
		if (comodo == 1) {
			System.out.println("Voê está na sala");						
		}else {
			System.out.println("");				
		}
		return sala;
		
			
	}
	
	public String getCozinha() {
		if (comodo == 2) {
			System.out.println("Você está na cozinha");
		}
		System.out.println("");
		return cozinha;		
	}
	
	public String getQuarto() {
		if (comodo == 3) {
			System.out.println("Você está no quarto");
		}
		System.out.println("");
		return quarto;	
	}
	
	public String getBanheiro() {
		if (comodo == 4) {
			System.out.println("Você está no Banheiro");			
		}
		System.out.println("");
		return banheiro;		
	}
	
	

}
