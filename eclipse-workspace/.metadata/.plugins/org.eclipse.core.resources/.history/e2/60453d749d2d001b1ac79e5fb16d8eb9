package brasil.br.com.bytebank.banco.teste;

public class Teste {

	public static void main(String[] args) {
		
		int [] idades = new int[5]; // ele inicia c/ zero
		
		idades[0] = 29;
		idades[1] = 39;
		idades[2] = 49;
		idades[3] = 59;
		idades[4] = 69;
		
		int idade4 = idades[4];
		
		System.out.println(idade4);
		System.out.println("Quntiades do Array " + idades.length);
		
	}

}
