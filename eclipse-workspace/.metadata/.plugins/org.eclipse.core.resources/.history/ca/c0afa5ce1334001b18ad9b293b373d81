package br.com.alura;

public class TestaCursoComAlunos {

	public static void main(String[] args) {
		
		Curso javaColecoes = new Curso("Dominando as coleções do java", "Vitor Hugo");		

		javaColecoes.adiciona(new Aula("Trabalhando com ArrayList", 21));
		javaColecoes.adiciona(new Aula("Criando uma Aula", 21));
		javaColecoes.adiciona(new Aula("Modelando com coleções", 22));
		
		Aluno a1 = new Aluno("Vitor Hugo", 0001);
		Aluno a2 = new Aluno("Larissa Martins", 0010);
		Aluno a3 = new Aluno("Allan Vinicius", 0020);
		Aluno a4 = new Aluno("Yuji", 0030);
		
		javaColecoes.matricular(a1);
		
		
	}
}
